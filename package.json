{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [
    "react boilerplate"
  ],
  "scripts": {
    "start": "webpack-dev-server --env.env=dev",
    "build": "webpack --env.env=prod",
    "lint": "npx eslint --ext .jsx,.js src/",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|scss|md)\"",
    "validate": "npm run lint && npm run prettier -- --list-different"
  },
  "repository": "git@github.com:Belzee10/react-boilerplate.git",
  "author": "Ivan <belzee92@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "favicons-webpack-plugin": "^1.0.2",
